journal = {International Journal of Learning Analytics and Artificial Intelligence for Education (iJAI)},
number = {1},
pages = {8},
title = {{Efforts in Europe for Data-Driven Improvement of Education – A Review of Learning Analytics Research in Seven Countries}},
